    				<inputToolkit:NumericUpDown Height="20" HorizontalAlignment="Left" Margin="0,2,8,2" Grid.Column="2" Grid.Row="10" Value="{Binding Mode=OneWay, Path=DisplayAdvancedConstraintsLevel}" Maximum="5"/>
    			</Grid>
    		</controls:TabItem>
    	</controls:TabControl>

    </Grid>
