    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="2" IsChecked="{Binding Mode=TwoWay, Path=HeroismAvailable}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="3" IsChecked="{Binding Mode=TwoWay, Path=PowerInfusionAvailable}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="4" IsChecked="{Binding Mode=TwoWay, Path=PotionOfWildMagic}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="5" IsChecked="{Binding Mode=TwoWay, Path=PotionOfSpeed}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="6" IsChecked="{Binding Mode=TwoWay, Path=FlameCap}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="7" IsChecked="{Binding Mode=TwoWay, Path=MaintainSnare}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="8" IsChecked="{Binding Mode=TwoWay, Path=MaintainScorch}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<ComboBox Margin="0,2,0,2" Grid.Column="1" Grid.Row="9" SelectedIndex="{Binding Mode=TwoWay, Path=HeroismControl}" Height="Auto">
    					<ListBoxItem Content="Optimal"/>
    					<ListBoxItem Content="Before 35%"/>
    					<ListBoxItem Content="No Cooldowns"/>
    					<ListBoxItem Content="After 35%"/>
    				</ComboBox>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=Innervate}" TextWrapping="Wrap" Grid.Row="10"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=ManaTide}" TextWrapping="Wrap" Grid.Row="11"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="12" IsChecked="{Binding Mode=TwoWay, Path=AverageCooldowns}"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="13" IsChecked="{Binding Mode=TwoWay, Path=DisableCooldowns}"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="14" IsChecked="{Binding Mode=TwoWay, Path=EvocationEnabled}"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="15" IsChecked="{Binding Mode=TwoWay, Path=ManaPotionEnabled}"/>
