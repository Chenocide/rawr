    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="2" IsChecked="{Binding Mode=TwoWay, Path=ReconstructSequence, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="3" IsChecked="{Binding Mode=TwoWay, Path=EnableHastedEvocation, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="4" IsChecked="{Binding Mode=TwoWay, Path=Mode32, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height, VerticalMargin"/>
    				<CheckBox Margin="0,2,8,2" Content="" Grid.Column="1" Grid.Row="8" IsChecked="{Binding Mode=TwoWay, Path=ComparisonIntegralMana, UpdateSourceTrigger=Default}" d:LayoutOverrides="Height" HorizontalAlignment="Right"/>
    				<ComboBox Margin="0,2,0,2" Grid.Column="1" Grid.Row="6" Height="Auto" Grid.ColumnSpan="2" SelectedItem="{Binding Mode=OneWay, Path=MIPMethod}">
