    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="1" Grid.Row="17" IsChecked="{Binding Mode=TwoWay, Path=CustomSpellMixEnabled}" Width="20" HorizontalAlignment="Left" VerticalAlignment="Center"/>
    				<Button Margin="0,2,0,2" Content="..." Grid.Column="1" Grid.Row="17" Width="96" HorizontalAlignment="Right" VerticalAlignment="Stretch" IsEnabled="False"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=SnaredTime}" TextWrapping="Wrap" Grid.Row="19"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=FocusMagicTargetCritRate}" TextWrapping="Wrap" Grid.Row="20"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=WarlockSpellPower}" TextWrapping="Wrap" Grid.Row="21"/>
    			</Grid>
    		</controls:TabItem>
    		<controls:TabItem Header="Fight">
