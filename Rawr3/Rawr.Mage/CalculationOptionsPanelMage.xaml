    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Segment Cooldowns: *" TextWrapping="Wrap" ToolTipService.ToolTip="Segment cooldowns into 30 second intervals." Grid.Row="9"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Advanced Constraints: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enables advanced constraints that are too expensive for comparisons, but are needed for sequence reconstruction." Grid.Row="10"/>
