    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Player Level: " TextWrapping="Wrap" ToolTipService.ToolTip=""/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Target Level: " TextWrapping="Wrap" ToolTipService.ToolTip="" Grid.Row="1"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fight Duration (sec):" TextWrapping="Wrap" ToolTipService.ToolTip="" Grid.Row="2"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Tps Limit: *" TextWrapping="Wrap" ToolTipService.ToolTip="Threat per second limit. Set to 0 to disable threat constraint." Grid.Row="3"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Dps time (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Time spent casting (default value is 1 = 100%). This value is analogous to Dps time in WWS reports, use it to indicate how much time is lost on movement and waiting." Grid.Row="4"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fragmentation (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Indicates how fragmented the dps time is and translates to how much out of five second rule time you get when not dpsing (higher value means more time inside five second rule)." Grid.Row="5"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Interrupt Frequency: *" TextWrapping="Wrap" ToolTipService.ToolTip="Frequency of pushbacks expressed as number of interrupts per second. An example value for Teron Gorefiend would be between 0.05 and 0.1." Grid.Row="6"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Molten Fury (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Percentage of time spent in molen fury range (i.e. &lt; 35% boss health). Default value of 0.3 indicates that 30% of time is spent in molten fury range due to raid dps stacking in execute range." Grid.Row="7"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Center" Width="Auto" Text="Aoe Target Level: *" TextWrapping="Wrap" ToolTipService.ToolTip="Level of aoe targets." Grid.Row="8"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Aoe Targets: *" TextWrapping="Wrap" ToolTipService.ToolTip="Number of simultaneous aoe targets." Grid.Row="9"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Aoe Duration (0-1): *" TextWrapping="Wrap" ToolTipService.ToolTip="Time spent aoeing. Value of 0.1 for example would mean 10% of time is spent aoeing." Grid.Row="10"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Arcane Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="11"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Fire Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="12"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Frost Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="13"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Nature Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="14"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Shadow Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="15"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Holy Resist: *" TextWrapping="Wrap" ToolTipService.ToolTip="Actual resistance of target, -1 for immunity." Grid.Row="16"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Drinking Time: *" TextWrapping="Wrap" ToolTipService.ToolTip="Setting this to a low value simulates having a limited time to regen mana on chain pulls (value is in seconds, optimizer has the option to continue drinking into the pull if it increases overall damage done, but that time counts toward total time)." Grid.Row="17"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Target Damage: *" TextWrapping="Wrap" ToolTipService.ToolTip="Instead of maximizing damage done this minimizes time it takes to deal the specified damage if value is greater than 0 (for best results enable Average Cooldowns)." Grid.Row="18"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Farming Mode: *" TextWrapping="Wrap" ToolTipService.ToolTip="Includes time it takes to regen mana/health after encounter, use in combination with target damage." Grid.Row="19"/>
    				<TextBlock Height="Auto" HorizontalAlignment="Stretch" Margin="2,2,0,2" VerticalAlignment="Top" Width="Auto" Text="Unlimited Mana: *" TextWrapping="Wrap" ToolTipService.ToolTip="Enable to completely ignore mana in evaluations." Grid.Row="20"/>
