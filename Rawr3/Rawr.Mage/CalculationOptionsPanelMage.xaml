    					<local:MIPMethod>HybridGuided</local:MIPMethod>
    					<local:MIPMethod>HybridUnguided</local:MIPMethod>
    				</ComboBox>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=AdditionalSegmentSplits, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.Row="13" Grid.ColumnSpan="2"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=LowerBoundHint, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.Row="14" Grid.ColumnSpan="2"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="0" IsChecked="{Binding Mode=TwoWay, Path=SmartOptimization, UpdateSourceTrigger=Default}"/>
    				<TextBox Height="20" Margin="0,2,0,2" Grid.Column="1" Text="{Binding Mode=TwoWay, Path=MaxHeapLimit, UpdateSourceTrigger=Default}" TextWrapping="Wrap" Grid.Row="5" Grid.ColumnSpan="2"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="8" IsChecked="{Binding Mode=TwoWay, Path=DisplayIntegralMana, UpdateSourceTrigger=Default}"/>
    				<CheckBox Margin="0,2,8,2" Content="" Grid.Column="1" Grid.Row="9" IsChecked="{Binding Mode=TwoWay, Path=ComparisonSegmentCooldowns, UpdateSourceTrigger=Default}" HorizontalAlignment="Right"/>
    				<CheckBox Margin="0,2,0,2" Content="" Grid.Column="2" Grid.Row="9" IsChecked="{Binding Mode=TwoWay, Path=DisplaySegmentCooldowns, UpdateSourceTrigger=Default}"/>
