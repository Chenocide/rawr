			<Layer level="OVERLAY">
				<Texture name="$parentBorderTexture" file="Interface\Addons\Rawr\Textures\Border">
					<Size x="41" y="41"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<NormalTexture/>
		<Scripts>
			<OnLoad>
				Rawr:ItemSlots_OnLoad(self);
			</OnLoad>
			<OnEnter>
				Rawr:ItemSlots_OnEnter(self);
			</OnEnter>
			<OnLeave>
				Rawr:ItemSlots_OnLeave(self);
			</OnLeave>
			<OnClick>
				Rawr:ItemSlots_OnClick(self,arg1);
			</OnClick>
		</Scripts>
	</Button>

